1、fastcgi
yum install spawn-fcgi
yum install fcgi fcgi-devel
config nginx 
cp src/fastcgi/nginx.conf /etc/nginx/
nginx -s reload

spawn-fcgi -a 127.0.0.1 -p 8888 -f ./demo -C 25 -F 4

curl -X GET 127.0.0.1/redis/

curl -u test:test -X GET http://127.0.0.1/exchange_rate/?source=USD\&target=CNY\&time=2016-07-04\&database=os

./weighttp -n 500000 -c 100 -t 2 -k 127.0.0.1/

./weighttp -n 500000 -c 100 -t 2 -k http://172.18.100.85:8088/apollo

ab -k -n 500000 -c 50 http://127.0.0.1/exchange_rate/

ab -r -k -n 500000 -c 100 http://127.0.0.1/redis/

ab -r -k -n 100000 -c 1000 http://127.0.0.1/redis/

ab -A test:test -k -n 500000 -c 100 http://127.0.0.1/redis/

siege -c 300 -r 500000 http://127.0.0.1/redis/

auth = echo -n 'test:test' | openssl base64
siege -t60s -c20 -d10 --header="Authorization:Basic test:test" 'http://127.0.0.1/redis/'





